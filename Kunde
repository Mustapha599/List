
public class Kunde {
    private String name;       // Name des Kunden

    // Standardkonstruktor
    public Kunde(String pName) {
        name = pName;
    }
    // Setzen des Namens
    public void setName(String pName) {
        name = pName;
    }

    // RÃ¼ckgabe des Namens
    public String getName() {
        return name;
    }
}

